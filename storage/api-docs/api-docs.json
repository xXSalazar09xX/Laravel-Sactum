                    "200": {
                        "description": "Ok",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "rows": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "id": {
                                                        "type": "number",
                                                        "example": "1"
                                                    },
                                                    "name": {
                                                        "type": "string",
                                                        "example": "dilan"
                                                    },
                                                    "email": {
                                                        "type": "string",
                                                        "example": "cris@gmail.com"
                                                    },
                                                    "email_varified_at": {
                                                        "type": "timestamp",
                                                        "example": ""
                                                    },
                                                    "password": {
                                                        "type": "string",
                                                        "example": "132135"
                                                    },
                                                    "estado": {
                                                        "type": "booleano",
                                                        "example": "0"
                                                    },
                                                    "tipo_id": {
                                                        "type": "foreignId",
                                                        "example": "2"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/registrarse": {
            "post": {
                "tags": [
                    "user"
                ],
                "summary": "Registo usuario",
                "operationId": "idcreate",
                "parameters": [
                    {
                        "name": "name",
                        "in": "path",
                        "description": "nombre del usuario",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "email",
                        "in": "path",
                        "description": "Correo",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "password",
                        "in": "path",
                        "description": "Contrasena",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "tipo_id",
                        "in": "path",
                        "description": "Tipo de usuario",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "ok"
                    },
                    "402": {
                        "description": "Campos requeridos"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "apiAuth": {
                "type": "http",
